    "@terrestris/ol-util": "^4.0.0",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.1.4",
    "@types/react-dom": "^16.9.3",
    "@types/react-rnd": "^8.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "proj4": "^2.6.0",
    "prop-types": "^15.7.2",
    "react-dom": "^16.12.0",
    "react-fa": "^5.0.0",
    "react-rnd": "^10.1.3",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-class-properties": "^7.8.0",
