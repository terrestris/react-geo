    "babel-plugin-import": "^1.13.0",
    "canvas": "^2.6.1",
    "copy-to-clipboard": "^3.2.1",
    "coveralls": "^3.0.9",
    "css-loader": "^4.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.2.0",
    "eslint-plugin-markdown": "^1.0.0",
