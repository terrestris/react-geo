    "@ant-design/icons": "^4.2.1",
    "@terrestris/base-util": "^0.2.3",
    "@terrestris/ol-util": "^4.0.1",
    "@types/lodash": "^4.14.155",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.0.13",
    "@types/react-dom": "^16.9.3",
    "@types/react-rnd": "^8.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "proj4": "^2.6.0",
    "prop-types": "^15.7.2",
    "react-dom": "^16.12.0",
    "react-fa": "^5.0.0",
    "react-rnd": "^10.2.1",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
