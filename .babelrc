{
  "presets": [
    "env",
    "stage-0",
    "react"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "build": {
      "plugins": [
        "babel-plugin-transform-import-extension-jsx-to-js"
      ]
    }
  },
  "plugins": [
    [
      "import", [{
          'libraryName': 'antd',
          'libraryDirectory': 'es',
          'style': true
        }, {
          'libraryName': '@turf/turf',
          'libraryDirectory': '../',
        }
      ]
    ]
  ]
}
