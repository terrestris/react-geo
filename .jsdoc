{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": "./src",
    "includePattern": ".js",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "destination": "build/docs/",
    "recurse": true,
    "verbose": true,
    "private": false,
    "template": "./node_modules/ink-docstrap/template"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "systemName": "react-geo",
    "copyright": "&copy; terrestris GmbH &amp; Co. KG, react-geo is released under the 2-Clause BSD License",
    "includeDate": true,
    "navType": "inline",
    "theme": "yeti",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": false,
    "outputSourceFiles": true ,
    "dateFormat": "YYYY-MM-DD",
    "syntaxTheme": "default",
    "search": true
  }
}
